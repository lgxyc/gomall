{{define "home"}}
{{template "header" .}}
    <div class="row">
    {{range .Items}}
        <div class="card col-xl-3 col-lg-4 col-md-6 col-sm-12 p-5 border-0">
            <img src="{{.Picture}}" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{ .Name}}</h5>
                <p class="card-text">{{ .Price}}</p>
            </div>
        </div>
    {{end}}
    </div>

{{template "footer"}}
{{end}}